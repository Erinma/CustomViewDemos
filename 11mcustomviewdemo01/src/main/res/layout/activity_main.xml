<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.a11mcustomviewdemo01.MainActivity">

    <com.example.a11mcustomviewdemo01.TitleView
        android:id="@+id/mytitle"
        android:layout_width="match_parent"
        android:layout_height="100dp"></com.example.a11mcustomviewdemo01.TitleView>

    <com.example.a11mcustomviewdemo01.PlusView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ></com.example.a11mcustomviewdemo01.PlusView>

    <com.example.a11mcustomviewdemo01.CustomView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
        android:visibility="gone"/>
    <com.example.a11mcustomviewdemo01.MutiTextView
        android:id="@+id/mymulti"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.example.a11mcustomviewdemo01.MutiTextView>
</LinearLayout>
